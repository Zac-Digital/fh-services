@page
@model IndexModel
@{
    ViewData["Title"] = "Manage family support accounts";
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">

        <h1 class="govuk-heading-l">Manage family support services and accounts</h1>

        <p>This is to manage family support accounts.</p>
        <p>Use this to:</p>
        <ul class="govuk-list govuk-list--bullet">
            <li>add DfE Admin</li>
            <li>change users permission and organisation</li>
        </ul>

        <a asp-page="/Welcome" class="govuk-button govuk-button--start" data-testid="start-button">
            Start now
            <svg class="govuk-button__start-icon" xmlns="http://www.w3.org/2000/svg" width="17.5" height="19" viewBox="0 0 33 40" aria-hidden="true" focusable="false">
                <path fill="currentColor" d="M0 0h13l20 20-20 20H0l20-20z"></path>
            </svg>
        </a>
    </div>

</div>
